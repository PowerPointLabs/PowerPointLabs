<UserControl x:Class="PowerPointLabs.TooltipsLab.Views.TooltipsLabSettingsAnimationEntry"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             d:DesignHeight="70" d:DesignWidth="50">
    <Grid x:Name="grid" ClipToBounds="True">
        <Image x:Name="imageBox" Margin="0,0,20,0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="50" Height="50"/>
        <TextBlock x:Name="textBlock" Margin="0,55,0,5" HorizontalAlignment="Left" Width="50" Height="10" TextTrimming="CharacterEllipsis" FontSize="8"/>
    </Grid>
</UserControl>