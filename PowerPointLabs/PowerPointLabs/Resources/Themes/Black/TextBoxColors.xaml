                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="TextBox.Static.Background" Color="#FF444444"/>
    <SolidColorBrush x:Key="TextBox.Static.Border" Color="#FF686868"/>
    <SolidColorBrush x:Key="TextBox.Static.Foreground" Color="#FFEEEEEE"/>
    <SolidColorBrush x:Key="TextBox.Static.CaretBrush" Color="#FFEEEEEE"/>
