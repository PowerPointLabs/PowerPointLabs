      <menuSeparator id="separatorSmartArt"/>
      <dynamicMenu id="MenuSmartArtBackground" getLabel="GetPowerPointLabsMenuLabel" getImage="GetContextMenuImage" getContent="GetContent"/>
    </contextMenu>
    <contextMenu idMso="ContextMenuSmartArtEditSmartArt">
      <menuSeparator id="separatorSmartArtEditSmartArt"/>
      <dynamicMenu id="MenuSmartArtEditSmartArt" getLabel="GetPowerPointLabsMenuLabel" getImage="GetContextMenuImage" getContent="GetContent"/>
    </contextMenu>
    <contextMenu idMso="ContextMenuSmartArtEditText">
      <menuSeparator id="separatorSmartArtEditText"/>
      <dynamicMenu id="MenuSmartArtEditText" getLabel="GetPowerPointLabsMenuLabel" getImage="GetContextMenuImage" getContent="GetContent"/>
    </contextMenu>
    
    <contextMenu idMso="ContextMenuCurve">
	    <menuSeparator id="separatorCurve" />
	    <menu id="MenuCurve" getLabel="GetPowerPointLabsMenuLabel" getImage="GetContextMenuImage" enabled="false">
	    </menu>
    </contextMenu>
    <contextMenu idMso="ContextMenuDrawnObject">
	    <menuSeparator id="separatorDrawnObject" />
	    <menu id="MenuDrawnObject" getLabel="GetPowerPointLabsMenuLabel" getImage="GetContextMenuImage" enabled="false">
	    </menu>
    </contextMenu>
    <contextMenu idMso="ContextMenuEndShow">
	    <menuSeparator id="separatorEndShow" />
